@model Web.ViewModel.TeamViewModel

<div class="form-group">
    <label asp-for="Team.Name" class="control-label"></label>
    <input asp-for="Team.Name" class="form-control" />
    <span asp-validation-for="Team.Name" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Team.LogoFile" class="control-label"></label>
    <input asp-for="Team.LogoFile" type="file" class="form-control" />
    <span asp-validation-for="Team.LogoFile" class="text-danger"></span>
</div>